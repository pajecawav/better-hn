{
	"private": true,
	"scripts": {
		"build": "cross-env FORCE_COLOR=1 run-s -l build:vite build:nitro",
		"build:vite": "vite build",
		"build:nitro": "nitro build",
		"dev": "cross-env FORCE_COLOR=1 run-p -l dev:*",
		"dev:vite": "vite --watch build",
		"dev:nitro": "cross-env NODE_OPTIONS=--enable-source-maps nitro dev",
		"preview": "cross-env NODE_OPTIONS=--enable-source-maps node .output/server/index.mjs",
		"lint": "cross-env FORCE_COLOR=1 run-p -l lint:*",
		"lint:eslint": "eslint .",
		"lint:tsc": "tsc --noEmit",
		"lint:format": "prettier . --check",
		"format": "prettier . --write",
		"clean": "rimraf .nitro .output",
		"prepare": "husky && nitropack prepare"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/js": "^9.29.0",
		"@pajecawav/prettier-config": "^1.0.0",
		"@types/js-cookie": "^3.0.6",
		"@vercel/analytics": "^1.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.29.0",
		"hono": "^4.7.11",
		"husky": "^9.1.7",
		"js-cookie": "^3.0.5",
		"lint-staged": "^16.1.1",
		"nitropack": "^2.11.12",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"sass": "^1.89.2",
		"tailwind-vars": "^0.1.6",
		"tiny-server-timing": "^0.1.5",
		"typescript-eslint": "^8.34.0",
		"vite": "^7.0.0",
		"vite-plugin-pwa": "^1.0.0"
	},
	"packageManager": "pnpm@10.11.0"
}
